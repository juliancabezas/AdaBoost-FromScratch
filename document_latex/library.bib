journal = {South African Journal Of Science},
number = {October},
pages = {467--473},
title = {{Consumption and direct-use values of savanna bio-resources used by rural households in Mametja , a semi-arid area of Limpopo province , South Africa}},
url = {http://researchspace.csir.co.za/dspace/handle/10204/1903},
volume = {99},
year = {2003}
