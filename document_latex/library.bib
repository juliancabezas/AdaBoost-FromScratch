issn = {09242716},
journal = {ISPRS Journal of Photogrammetry and Remote Sensing},
keywords = {ASTER GDEM2,RTK-GNSS,SRTM,Savanna,TanDEM-X IDEM,Validation},
pages = {496--508},
publisher = {International Society for Photogrammetry and Remote Sensing, Inc. (ISPRS)},
title = {{TanDEM-X IDEM precision and accuracy assessment based on a large assembly of differential GNSS measurements in Kruger National Park, South Africa}},
