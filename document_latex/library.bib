number = {2},
pages = {465--474},
title = {{Mapping wetlands and riparian areas using Landsat ETM+ imagery and decision-tree-based models}},
url = {http://link.springer.com/10.1672/0277-5212(2006)26%5B465%3AMWARAU%5D2.0.CO%3B2},
