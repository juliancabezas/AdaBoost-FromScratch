file = {:home/julian/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/S{\o}rensen, Zinko, Seibert - 2006 - On the calculation of the topographic wetness index evaluation of different methods based on field ob.pdf:pdf},
isbn = {1027-5606},
