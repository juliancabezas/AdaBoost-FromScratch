file = {:home/julian/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Tadono et al. - 2016 - Generation of the 30 M-MESH global digital surface model by alos prism.pdf:pdf},
issn = {16821750},
journal = {International Archives of the Photogrammetry, Remote Sensing and Spatial Information Sciences - ISPRS Archives},
keywords = {DEM/DTM,Global,High resolution,Mapping,Satellite},
number = {July},
