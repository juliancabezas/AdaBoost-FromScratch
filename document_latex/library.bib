isbn = {9781479957750},
issn = {01962892},
journal = {IEEE Transactions on Geoscience and Remote Sensing},
keywords = {Benchmarking,component substitution (CS),multiresolution analysis (MRA),multispectral (MS) pansharpening,quality assessment,very high-resolution optical images},
number = {5},
pages = {2565--2586},
title = {{A critical comparison among pansharpening algorithms}},
