journal = {Remote Sensing of Environment},
keywords = {Biodiversity patterns,Landscape change,Life-forms,Mediterranean vegetation,QuickBird,Regression trees,Spatial dependence,Texture},
pages = {208--221},
publisher = {Elsevier Inc.},
title = {{Modeling plant species richness using reflectance and texture data derived from QuickBird in a recently burned area of Central Spain}},
url = {http://dx.doi.org/10.1016/j.rse.2011.12.024},
volume = {119},
