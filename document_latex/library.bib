journal = {Applied Geomatics},
keywords = {90mSRTM DEM,Delineation,Flat-steeper slope,Morphological parameters,River network,Spatial-deviated alignment},
number = {2},
pages = {49--58},
title = {{Limitation of 90 m SRTM DEM in drainage network delineation using D8 method-a case study in flat terrain of Bangladesh}},
