file = {:home/julian/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Pushparaj, Hegde - 2017 - Comparison of various pan-sharpening methods using Quickbird-2 and Landsat-8 imagery.pdf:pdf},
isbn = {1251701728},
issn = {18667538},
journal = {Arabian Journal of Geosciences},
keywords = {Fusion methods,Multispectral imagery,Pan-sharpening,Qualitative analysis,Quantitative analysis},
number = {5},
title = {{Comparison of various pan-sharpening methods using Quickbird-2 and Landsat-8 imagery}},
