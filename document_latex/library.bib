@article{Aartsen2015,
abstract = {We present a measurement of neutrino oscillations via atmospheric muon neutrino disappearance with three years of data of the completed IceCube neutrino detector. DeepCore, a region of denser IceCube instrumentation, enables the detection and reconstruction of atmospheric muon neutrinos between 10 and 100 GeV, where a strong disappearance signal is expected. The IceCube detector volume surrounding DeepCore is used as a veto region to suppress the atmospheric muon background. Neutrino events are selected where the detected Cherenkov photons of the secondary particles minimally scatter, and the neutrino energy and arrival direction are reconstructed. Both variables are used to obtain the neutrino oscillation parameters from the data, with the best fit given by $\Delta$m322=2.72-0.20+0.19×10-3eV2 and sin2$\theta$23=0.53-0.12+0.09 (normal mass ordering assumed). The results are compatible, and comparable in precision, to those of dedicated oscillation experiments.},
