isbn = {1146-609X},
issn = {1146609X},
journal = {Acta Oecologica},
keywords = {GIS,Quickbird,Species richness,Spectral variation hypothesis,Wetland},
number = {2},
pages = {117--120},
title = {{Testing the spectral variation hypothesis by using satellite multispectral images}},
volume = {26},
year = {2004}
