issn = {18666299},
journal = {Environmental Earth Sciences},
keywords = {Atmospheric temperature,Soil wetness,Surface temperature,Urban climate,Urban heat island intensity},
number = {5},
title = {{Spatiotemporal analysis of the atmospheric and surface urban heat islands of the Metropolitan Area of Toluca, Mexico}},
volume = {76},
year = {2017}
