@article{Goodenough2012a,
abstract = {Despite recent papers on problems associated with full-model and stepwise regression, their use is still common throughout ecological and environmental disciplines. Alternative approaches, including generating multiple models and comparing them post-hoc using techniques such as Akaike's Information Criterion (AIC), are becoming more popular. However, these are problematic when there are numerous independent variables and interpretation is often difficult when competing models contain many different variables and combinations of variables. Here, we detail a new approach, REVS (Regression with Empirical Variable Selection), which uses all-subsets regression to quantify empirical support for every independent variable. A series of models is created; the first containing the variable with most empirical support, the second containing the first variable and the next most-supported, and so on. The comparatively small number of resultant models (n = the number of predictor variables) means that post-hoc comparison is comparatively quick and easy. When tested on a real dataset--habitat and offspring quality in the great tit (Parus major)--the optimal REVS model explained more variance (higher R(2)), was more parsimonious (lower AIC), and had greater significance (lower P values), than full, stepwise or all-subsets models; it also had higher predictive accuracy based on split-sample validation. Testing REVS on ten further datasets suggested that this is typical, with R(2) values being higher than full or stepwise models (mean improvement = 31% and 7%, respectively). Results are ecologically intuitive as even when there are several competing models, they share a set of "core" variables and differ only in presence/absence of one or two additional variables. We conclude that REVS is useful for analysing complex datasets, including those in ecology and environmental disciplines.},
author = {Goodenough, Anne E and Hart, Adam G and Stafford, Richard},
doi = {10.1371/journal.pone.0034338},
file = {:home/julian/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Goodenough, Hart, Stafford - 2012 - Regression with empirical variable selection description of a new method and application to ecologic.pdf:pdf},
issn = {1932-6203},
journal = {PloS one},
keywords = {Animals,Birds,Databases, Factual,Ecology,Ecology: methods,Models, Biological,Models, Statistical,Regression Analysis},
month = {jan},
number = {3},
pages = {e34338},
pmid = {22479605},
title = {{Regression with empirical variable selection: description of a new method and application to ecological datasets.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3316704&tool=pmcentrez&rendertype=abstract},
volume = {7},
year = {2012}
}
