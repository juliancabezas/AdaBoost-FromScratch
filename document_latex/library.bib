file = {:home/julian/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Hais et al. - 2009 - Comparison of two types of forest disturbance using multitemporal Landsat TMETM imagery and field vegetation data.pdf:pdf},
isbn = {0034-4257},
issn = {00344257},
journal = {Remote Sensing of Environment},
keywords = {Bark beetle,Change detection,Clear-cut,Disturbance,Landsat},
number = {4},
pages = {835--845},
pmid = {19672320},
title = {{Comparison of two types of forest disturbance using multitemporal Landsat TM/ETM+ imagery and field vegetation data}},
volume = {113},
year = {2009}
}
